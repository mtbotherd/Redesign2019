<!DOCTYPE html>
<html lang="en">

<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
	<title>Metro Transit - Website Design Guide</title>
	<link rel="shortcut icon" href="favicon.ico" type="image/x-icon" />
	<!-- build:css app.min.css -->
	<link rel="stylesheet" href="app.css" />
	<!-- endbuild -->
</head>

<body>

	<!-- Begin Header -->
	<partial src="partials/header.html" />
	<!-- End Header -->

	<!-- Begin Main Content -->
	<section id="mainContent">
		<div id="secondaryPage">
			<div class="page-title">
				<div class="container">
					<h1>Trip Plan</h1>
				</div>
			</div>
			<div id="tripPlan">
				<div class="container">
					<div class="tt-trip-plan">
				        <div class="card">
				            <div class="card-header">
				                <span class="d-flex">
				                    <span class="d-flex align-items-center tp-time">
				                        56&nbsp;
				                        <abbr title="minutes">min</abbr>
				                    </span>
				                    <span class="d-flex align-items-center tp-route">
				                        <img class="icon" src="/img/svg/pedestrian-gray.svg">
				                        <img class="icon chevron-right-gray mr-2" src="/img/svg/chevron-right-gray.svg">
				                        <img class="icon" src="/img/svg/bus-gray.svg">&nbsp;
				                        <span class="route mr-1">21</span>
				                        <img class="icon chevron-right-gray mr-1" src="/img/svg/chevron-right-gray.svg">
				                        <span class="d-flex align-items-center badge badge-success mr-1">
				                            <img class="icon" src="/img/svg/lrt-white.svg">
				                            <span class="caps">Green</span>
				                        </span>
				                        <img class="icon chevron-right-gray" src="/img/svg/chevron-right-gray.svg">
				                        <img class="icon mr-1" src="/img/svg/pedestrian-gray.svg">
										<img class="icon ml-auto" src="/img/svg/link-gray.svg">
				                    </span>
				                </span>
				            </div>
				            <div class="card-body border-top-0">
				                <div class="row flex-row">
				                    <div class="col-lg-5">
				                        <div class="d-block tp-results">
				                            <div class="leg-item">
				                                <div class="d-table-cell leg-time">1:06 p.m.</div>
				                                <div class="d-table-cell leg-mode walk">
				                                    <div class="d-table-cell leg-mode-icon">
				                                        <img class="icon" src="/img/svg/circle-green-outline-pedestrian.svg">
				                                    </div>
				                                    <p>
				                                        <strong>Walk</strong> 0.12 miles to Selby
				                                        <abbr title="avenue">Ave</abbr> &amp; Farrington
				                                        <abbr title="street">St</abbr>
				                                        <br>
				                                        <small>
				                                            (about 4
				                                            <abbr title="minutes">min</abbr>)
				                                        </small>
				                                    </p>
				                                </div>
				                            </div>
				                            <div class="leg-item">
				                                <div class="d-table-cell leg-time">1:10 p.m.</div>
				                                <div class="d-table-cell leg-mode bus">
				                                    <div class="d-table-cell leg-mode-icon">
				                                        <img class="icon" src="/img/svg/circle-gray-outline-bus.svg">
				                                    </div>
				                                    <p>
				                                        <img class="icon blink" src="/img/svg/broadcast-red.svg">&nbsp;<strong>Currently 5
				                                        <abbr title="minutes">min</abbr> late</strong>
				                                        <br>
				                                        Route 21 Selby-Lake/Uptown
				                                        <br>
				                                        <a href="/home/#ServiceAlerts">
				                                            <small>view alerts</small>
				                                        </a>
				                                    </p>
				                                    <p>
				                                        <strong>Depart</strong> from Selby
				                                        <abbr title="avenue">Ave</abbr> &amp; Farrington
				                                        <abbr title="street">St</abbr>
				                                    </p>
				                                    <p>
				                                        <strong>Arrive</strong> at Snelling
				                                        <abbr title="avenue">Ave</abbr> &amp; University
				                                        <abbr title="avenue">Ave</abbr> / SpruceTree
				                                        <abbr title="drive">Dr</abbr>
				                                    </p>
				                                </div>
				                            </div>
				                            <div class="leg-item">
				                                <div class="d-table-cell leg-time">1:40 p.m.</div>
				                                <div class="d-table-cell leg-mode metro-green">
				                                    <div class="d-table-cell leg-mode-icon">
				                                        <img class="icon" src="/img/svg/circle-green-outline-lrt.svg">
				                                    </div>
				                                    <p>
				                                        <strong>Green Line</strong> to
				                                        <abbr title="Minneapolis">Mpls</abbr>-Target Field
				                                    </p>
				                                    <p>
				                                        <strong>Depart</strong> from Snelling
				                                        <abbr title="avenue">Ave</abbr> Station
				                                    </p>
				                                    <p>
				                                        <strong>Arrive</strong> at Target Field Station Platform 1
				                                    </p>
				                                </div>
				                            </div>
				                            <div class="leg-item">
				                                <div class="d-table-cell leg-time">1:10 p.m.</div>
				                                <div class="d-table-cell leg-mode walk">
				                                    <div class="d-table-cell leg-mode-icon">
				                                        <img class="icon" src="/img/svg/circle-green-outline-pedestrian.svg">
				                                    </div>
				                                    <p>
				                                        <strong>Walk</strong> 0.25 miles
				                                        <abbr title="west">W</abbr> to
				                                        <abbr title="Fred T. Heywood">FTH</abbr>, Minneapolis, MN
				                                        <br>
				                                        <small>
				                                            (about 6
				                                            <abbr title="minutes">min</abbr>)
				                                        </small>
				                                    </p>
				                                </div>
				                            </div>
				                            <div class="leg-item">
				                                <div class="d-table-cell leg-time">12:88 p.m.</div>
				                                <div class="d-table-cell leg-mode arrive">
				                                    <div class="d-table-cell leg-mode-icon">
				                                        <img class="icon" src="/img/svg/circle-red-outline-pin.svg">
				                                    </div>
				                                    <p>
				                                        <strong>Arrive</strong>
				                                        at
				                                        <abbr
				                                            title="Fred T. Heywood"
				                                        >FTH</abbr>, Minneapolis, MN
				                                    </p>
				                                </div>
				                            </div>
				                        </div>
				                        <div class="clearfix"></div>
				                        <hr class="d-block d-lg-none">
				                    </div>
				                    <div class="col-lg-7">
				                        <div class="tp-basemap">
				                            <div class="card border">
				                                <img class="img-fluid" src="/img/TripPlanMap1.png">
				                            </div>
				                        </div>
				                    </div>
				                </div>
				            </div>
				        </div>
					</div>
				</div>
			</div>
		</div>
    </section>

    <!-- End Main Content -->

	<!-- Begin Footer -->
	<partial src="partials/footer.html" />
	<!-- End Footer -->

	<!-- Vendor scripts -->
	<partial src="partials/vendor-scripts.html" />
	<!-- End vendor scripts -->
	<!-- build:js js/app.min.js -->
	<script src="js/scripts.js"></script>
	<!-- endbuild -->
</body>

</html>
